{
  "name": "@inversiland/inversify",
  "type": "module",
  "version": "6.1.5",
  "description": "A powerful and lightweight inversion of control container for JavaScript and Node.js apps powered by TypeScript.",
  "author": {
    "name": "Carlos Sala Samper",
    "url": "https://godofprogramming.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/inversiland/inversiland.git",
    "directory": "/packages/inversify"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "dependency injection",
    "dependency inversion",
    "di",
    "inversion of control container",
    "ioc",
    "javascript",
    "node",
    "typescript"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf amd es es6 lib",
    "postupdate": "git diff-files --quiet package-lock.json || npm test",
    "prepublish": "npm run build && publish-please guard",
    "publish-please": "publish-please",
    "test": "jest --coverage && jest-coverage-badges --input ./coverage/coverage-summary.json --output ./assets/badges/coverage",
    "update": "updates --update --minor && npm install"
  },
  "dependencies": {
    "@inversifyjs/common": "1.3.3",
    "@inversifyjs/core": "1.3.4"
  },
  "devDependencies": {
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "ts-loader": "9.5.1",
    "updates": "16.4.0",
    "webpack": "5.96.1",
    "webpack-cli": "5.1.4"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.2.2"
  }
}
